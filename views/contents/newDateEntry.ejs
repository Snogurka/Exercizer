<!-- new entry date section -->
<section class="workoutEntry">
  <div class="row">
    <label for="entryDate" class="col4">New Date</label>
    <input type="date" name="entryDate" class="col4" dateonly=true required onblur="checkDateUniqueness(event.target)">
    <div class="col3"></div>
    <button type="button" class="col1 emoj" onclick="showEmojis(event.target)">😎</button>
  </div>

<!-- to warn user of existing record dates, pull recorded dates  into a hidden array; todo: sometime later, limit the dates in calendar to only those that have entries -->
<input type="hidden" id="recordedDates" value="<%= entryDates %> ">
<%- include ("newWorkoutEntry.ejs") %>
<%- include("../partials/alert.ejs") %>
</section>